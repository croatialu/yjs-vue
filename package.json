{
  "name": "pkg-placeholder",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "bun@1.1.2",
  "description": "_description_",
  "author": "Croatia Lu <wuli.croatia@foxmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/croatialu",
  "homepage": "https://github.com/croatialu/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/croatialu/pkg-placeholder.git"
  },
  "bugs": "https://github.com/croatialu/pkg-placeholder/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run ./scripts/build.ts",
    "dev": "bun run build --watch",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "bun run --watch ./src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/bun": "^1.1.0",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "bun-plugin-dts": "^0.2.3",
    "eslint": "^9.0.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "bunx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
